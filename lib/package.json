        "postcss-normalize": "^10.0.1",
        "postcss-preset-env": "^7.2.0",
        "rollup": "^3.26.2",
        "rollup-plugin-copy": "^3.4.0",
        "rollup-plugin-filesize": "^10.0.0",
        "rollup-plugin-postcss": "^4.0.2",
        "stylelint": "^14.2.0",
