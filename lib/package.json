        "prepack": "yarn build",
        "typecheck": "tsc",
        "check-unused": "unimported",
        "test": "jest",
        "test:coverage": "jest --coverage",
        "lint": "eslint ./src --report-unused-disable-directives",
        "css-lint": "stylelint 'src/**/*.css' --config .stylelintrc.json"
